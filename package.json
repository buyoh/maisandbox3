{
  "name": "maisandbox3",
  "private": true,
  "main": "/dev/null",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/app/MainServer.ts ",
    "prod": "NODE_ENV=production node build/app/MainServer.js",
    "build": "yarn build-next && yarn build-server",
    "build-next": "next build",
    "build-server": "tsc --project tsconfig.server.json",
    "test": "jest --coverage",
    "test-update": "jest --updateSnapshot",
    "old-test": "yarn run test-server && yarn run test-next",
    "old-test-server": "TS_NODE_PROJECT=tsconfig.server.json TEST_TARGET='**/src/test/app/' jest --coverage",
    "old-test-next": "TS_NODE_FILES=true TEST_TARGET='**/src/test/web/' jest --coverage",
    "lint": "prettier --check 'src/**/*.ts' 'src/**/*.tsx' && eslint src/**/*.ts src/**/*.tsx",
    "lint-fix": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.18",
    "eslint-config-prettier": "^6.13.0",
    "express": "^4.17.1",
    "next": "9.4.4",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-ace": "9.1.4",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.4",
    "redux": "^4.0.5",
    "sass": "^1.26.12",
    "socket.io": "^3.0.5",
    "socket.io-client": "^3.0.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.0.27",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "@types/react-test-renderer": "^16.9.5",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.6.0",
    "eslint-plugin-jest": "^24.1.10",
    "eslint-plugin-react": "^7.20.6",
    "ignore-styles": "^5.0.1",
    "jest": "^26.5.3",
    "nodemon": "^2.0.12",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.10.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.10",
    "yarn": "^1.22.11"
  }
}
