{
  "name": "maisandbox3",
  "private": true,
  "main": "/dev/null",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/app/MainServer.ts ",
    "prod": "NODE_ENV=production node build/app/MainServer.js",
    "build": "yarn build-next && yarn build-server",
    "build-next": "next build",
    "build-server": "tsc --project tsconfig.server.json",
    "test": "jest --coverage",
    "old-test": "yarn run test-server && yarn run test-next",
    "old-test-server": "TS_NODE_PROJECT=tsconfig.server.json TEST_TARGET='**/src/test/app/' jest --coverage",
    "old-test-next": "TS_NODE_FILES=true TEST_TARGET='**/src/test/web/' jest --coverage",
    "lint": "prettier --check 'src/**/*.ts' 'src/**/*.tsx' && eslint src/**/*.ts src/**/*.tsx",
    "lint-fix": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "eslint-config-prettier": "^6.13.0",
    "express": "^4.17.1",
    "next": "9.4.2",
    "prettier": "^2.1.2",
    "react": "16.13.1",
    "react-ace": "9.1.4",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "sass": "^1.26.5",
    "socket.io": "^3.0.3",
    "socket.io-client": "^3.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.3",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.20.5",
    "ignore-styles": "^5.0.1",
    "jest": "^26.5.3",
    "nodemon": "^2.0.4",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.3",
    "yarn": "^1.22.4"
  }
}
