{
  "name": "maisandbox3",
  "private": true,
  "main": "/dev/null",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/app/MainServer.ts ",
    "prod": "NODE_ENV=production node build/app/MainServer.js",
    "build": "yarn build-next && yarn build-server",
    "build-next": "next build",
    "build-server": "tsc --project tsconfig.server.json",
    "test": "jest --coverage",
    "test-update": "jest --updateSnapshot",
    "old-test": "yarn run test-server && yarn run test-next",
    "old-test-server": "TS_NODE_PROJECT=tsconfig.server.json TEST_TARGET='**/src/test/app/' jest --coverage",
    "old-test-next": "TS_NODE_FILES=true TEST_TARGET='**/src/test/web/' jest --coverage",
    "lint": "prettier --check 'src/**/*.ts' 'src/**/*.tsx' && eslint src/**/*.ts src/**/*.tsx",
    "lint-fix": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "eslint-config-prettier": "^6.15.0",
    "express": "^4.18.2",
    "next": "11.1.2",
    "prettier": "^2.7.1",
    "react": "^16.14.0",
    "react-ace": "9.1.4",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.9",
    "redux": "^4.2.0",
    "sass": "^1.55.0",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^26.0.24",
    "@types/node": "^18.11.9",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^17.0.2",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-react": "^7.31.10",
    "ignore-styles": "^5.0.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.20",
    "react-test-renderer": "^17.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4",
    "yarn": "^1.22.19"
  }
}
