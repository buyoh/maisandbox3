{
  "name": "maisandbox3",
  "private": true,
  "main": "/dev/null",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json app/MainServer.ts ",
    "prod": "NODE_ENV=production node build/app/MainServer.js",
    "build": "yarn build-next && yarn build-server",
    "build-next": "next build",
    "build-server": "tsc --project tsconfig.server.json",
    "test": "yarn run test-server && yarn run test-next",
    "test-server": "TS_NODE_PROJECT=tsconfig.server.json mocha --require ts-node/register mocha/server/**/*Test.ts",
    "test-next": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\", \"jsx\": \"react\"}' mocha --require ts-node/register  mocha/next/**/*Test.{ts,tsx}",
    "lint": "eslint components/**/*.ts components/**/*.tsx pages/**/*.ts pages/**/*.tsx mocha/**/*.ts app/**/*.ts",
    "lint-fix": "eslint --fix components/**/*.ts components/**/*.tsx pages/**/*.ts pages/**/*.tsx mocha/**/*.ts app/**/*.ts"
  },
  "dependencies": {
    "express": "^4.17.1",
    "next": "9.4.2",
    "react": "16.13.1",
    "react-ace": "^9.0.0",
    "react-dom": "16.13.1",
    "sass": "^1.26.5",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.6",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "mocha": "^8.0.1",
    "mocha-jsdom": "^2.0.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.3",
    "yarn": "^1.22.4"
  }
}
