{
  "name": "maisandbox3",
  "private": true,
  "main": "/dev/null",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/app/MainServer.ts ",
    "prod": "FRONTEND=static NODE_ENV=production node build/app/MainServer.js",
    "build": "yarn build-vite && yarn build-server",
    "build-vite": "vite build",
    "build-server": "tsc --project tsconfig.server.json",
    "test": "jest --coverage",
    "test-update": "jest --updateSnapshot",
    "lint": "prettier --check 'src/**/*.ts' 'src/**/*.tsx' && eslint src/**/*.ts src/**/*.tsx",
    "lint-fix": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "eslint-config-prettier": "^6.15.0",
    "express": "^4.18.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "redux": "^4.2.1",
    "sass": "^1.55.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^26.0.24",
    "@types/node": "^18.11.19",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^17.0.9",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^26.6.3",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4",
    "vite": "^5.1.4"
  },
  "packageManager": "yarn@4.1.1"
}
